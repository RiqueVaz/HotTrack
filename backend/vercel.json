{
  "version": 2,
  "builds": [
    { "src": "server.js", "use": "@vercel/node" },
    { "src": "worker/process-timeout.js", "use": "@vercel/node" }
  ],
  "rewrites": [
    { 
      "source": "/api/worker/process-timeout", 
      "destination": "/worker/process-timeout.js" 
    },
    { 
      "source": "/api/(.*)", 
      "destination": "/server.js" 
    }
  ]
}